{
  "version": 1,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "functions": {
    "main.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "crons": [],
  "outputDirectory": "vercel/output",
  "env": {
    "PYTHONPATH": "./"
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "framework": "fastapi"
}